{
  "If": {
    "prefix": "i",
    "body": [
      "if (${condition}) {\n  ${0}\n}"
    ],
    "description": "If statement"
  },

	"Lambda": {
    "prefix": "f",
    "body": [
      "(${1}) => {$0}"
    ],
    "description": "Lambda function"
  },

  "ConsoleLog": {
    "prefix": "l",
    "body": [
      "console.log($1);$0"
    ],
    "description": "Console log"
  },

  "Promise": {
    "prefix": "p",
    "body": [
      "new Promise((resolve, ${1:reject}) => {",
      "  $2",
      "})$0"
    ],
    "description": "Promise wrap"
  }
}